      <TableDataSource Name="MovimentosItens" DataType="System.Int32" Enabled="true" TableName="MovimentosItens" SelectCommand="SELECT c.Nome,&#13;&#10;c.Morada,&#13;&#10;m.ClienteID,&#13;&#10;m.DataCriacao,&#13;&#10;mi.idMovimento,&#13;&#10;mi.idProduto,&#13;&#10;p.Nome as ProdutoNome,&#13;&#10;mi.Preco,&#13;&#10;mi.Quantidade,&#13;&#10;CASE mi.Tipo&#13;&#10;    WHEN 0 THEN 'Venda'&#13;&#10;    WHEN 1 THEN 'Empréstimo'&#13;&#10;    WHEN 2 THEN 'Oferta'&#13;&#10;    ELSE 'Desconhecido'&#13;&#10;END AS 'TipoDesc'&#13;&#10;FROM MovimentosItens mi&#13;&#10;INNER JOIN Movimentos m&#13;&#10;ON m.id = mi.idMovimento&#13;&#10;INNER JOIN Clientes c&#13;&#10;ON c.id = m.ClienteID&#13;&#10;INNER JOIN Produtos p&#13;&#10;ON p.id = mi.idProduto&#13;&#10;where mi.idMovimento = @idMovimento">
